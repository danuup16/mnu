local detectedEvents = {}
local selectedEvent = nil
local EventDropdown = nil
local eventCoordinates = {
    ["Shark Hunt"] = {
        Vector3.new(1.64999, 5, 2095.72),
        Vector3.new(1369.94, 5, 930.125),
        Vector3.new(-1585.5, 5, 1242.87),
