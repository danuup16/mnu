-- Variabel Webhook
local webhookURL = ""
local isWebhookActive = false
local lastInventoryState = {}
local inventoryConnection = nil
local httpRetryCount = 0
local maxHttpRetries = 3

-- Ikan yang Diaktifkan (Hanya ikan ini yang akan dilacak)
