        FrostbornSharkToggle:Set(false)
        -- Mythic
        DottedStingrayToggle:Set(false)
        AbyssSeahorseToggle:Set(false)
        HawksTurtleToggle:Set(false)
        HammerheadSharkToggle:Set(false)
        MantaRayToggle:Set(false)
        PrismySeahorseToggle:Set(false)
        LoggerheadTurtleToggle:Set(false)
        BlueflameRayToggle:Set(false)
        -- Legendary
        YellowfinTunaToggle:Set(false)
        ChromeTunaToggle:Set(false)
        LobsterToggle:Set(false)
        BumblebeeGrouperToggle:Set(false)
        LavafinTunaToggle:Set(false)
        BlueLobsterToggle:Set(false)
        GreenbeeGrouperToggle:Set(false)
        StarjamTangToggle:Set(false)
        MagicTangToggle:Set(false)
        EnchantedAngelfishToggle:Set(false)
        -- Uncommon
        PilotFishToggle:Set(false)
        BlackcapBassletToggle:Set(false)
        SilverTunaToggle:Set(false)
